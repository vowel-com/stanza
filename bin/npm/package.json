{
    "name": "@vowel-com/stanza",
    "description": "Modern XMPP in the browser, with a JSON API",
    "version": "0.1.2",
    "author": "Lance Stout <lancestout@gmail.com>",
    "bugs": "https://github.com/vowel-com/stanza/issues",
    "contributors": [
        "Philipp Hancke <fippo@andyet.net>",
        "Steven Lloyd Watkin <lloyd@evilprofessor.co.uk>"
    ],
    "dependencies": {
        "@types/async": "^3.0.3",
        "@types/node": "^12.12.20",
        "@types/punycode": "^2.1.0",
        "@types/readable-stream": "^2.3.5",
        "@types/ws": "^6.0.4",
        "async": "^3.0.1",
        "node-fetch": "^2.6.0",
        "punycode": "^2.1.1",
        "react-native-randombytes": "^3.5.3",
        "readable-stream": "^2.3.6",
        "sdp": "^3.0.2",
        "stanza-shims": "^1.1.1",
        "tslib": "^1.9.3",
        "ws": "^7.2.1"
    },
    "homepage": "https://stanzajs.org",
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --stage && npm run lint && npm test"
        }
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testRegex": "test/.*\\.ts$",
        "testMatch": null,
        "collectCoverage": true,
        "collectCoverageFrom": [
            "./src/**/*.ts"
        ],
        "coverageDirectory": "./coverage",
        "coverageReporters": [
            "text",
            "html"
        ],
        "testTimeout": 10000
    },
    "keywords": [
        "jingle",
        "stanza",
        "stanza.io",
        "xmpp"
    ],
    "license": "MIT",
    "main": "./index.js",
    "prettier": {
        "tabWidth": 4,
        "printWidth": 100,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "none",
        "arrowParens": "avoid"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "repository": "git://github.com/vowel-com/stanza.git",
    "browser": "./module.js",
    "module": "./module.js",
    "private": false,
    "react-native": "./index.js",
    "sideEffects": false,
    "typings": "./index"
}
